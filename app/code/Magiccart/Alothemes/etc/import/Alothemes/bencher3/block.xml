      <identifier><![CDATA[cms-static-site-map]]></identifier>
      <content><![CDATA[<div id="cms-static-site-map" class="cms-static-site-map site-map">
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script>
<div style="overflow: hidden; height: 450px; width: 1920px;">
<div id="gmap_canvas" style="height: 450px; width: 1920px;">&nbsp;</div>
<div><small><a href="http://embedgooglemaps.com"> embed google maps </a></small></div>
<div><small><a href="#">free web directories</a></small></div>
</div>
</div>
<script type="text/javascript">function init_map(){var myOptions = {zoom:10,center:new google.maps.LatLng(51.48084576284743,-0.1648371574218599),mapTypeId: google.maps.MapTypeId.ROADMAP};map = new google.maps.Map(document.getElementById('gmap_canvas'), myOptions);marker = new google.maps.Marker({map: map,position: new google.maps.LatLng(51.48084576284743,-0.1648371574218599)});infowindow = new google.maps.InfoWindow({content:'<a href="#"><img class="img-responsive" alt="" src="{{media url="wysiwyg/alothemes/static/site-map.jpg"}}" /></a><strong>Location</strong><br>Bencher London oxford street 02 united kingdom<br>'});google.maps.event.addListener(marker, 'click', function(){infowindow.open(map,marker);});infowindow.open(map,marker);}google.maps.event.addDomListener(window, 'load', init_map);</script>]]></content>
